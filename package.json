{
	"name": "log-tool",
	"version": "1.0.0",
	"description": "Log Tool",
	"author": {
		"name": "Daniel Bar"
	},
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"preinstall": "is-ci || npx only-allow yarn",
		"husky:install": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"prepare": "is-ci || run-p husky:install git:config",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "run-p ts-lint styelint",
		"eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"eslint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"stylelint": "stylelint \"src/**/*.scss\"",
		"stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"depcheck": "depcheck --config .depcheckrc.json",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@testing-library/jest-dom": "^5.15.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "5.0.1",
		"rsuite": "5.14.0",
		"web-vitals": "2.1.4"
	},
	"devDependencies": {
		"@commitlint/cli": "16.3.0",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.11",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"commitizen": "4.2.4",
		"cz-vinyl": "1.3.2",
		"depcheck": "1.4.3",
		"eslint": "8.16.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.5.0",
		"eslint-plugin-unused-imports": "2.0.0",
		"husky": "8.0.1",
		"hygen": "^6.1.0",
		"is-ci": "3.0.1",
		"lint-staged": "12.4.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.0",
		"stylelint": "^14.3.0",
		"stylelint-config-prettier-scss": "0.0.1",
		"stylelint-config-recess-order": "3.0.0",
		"stylelint-config-standard-scss": "^3.0.0",
		"typescript": "^4.5.2"
	}
}
