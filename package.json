{
	"name": "log-tool",
	"version": "1.0.0",
	"description": "Log Tool",
	"author": {
		"name": "Daniel Bar"
	},
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"preinstall": "is-ci || npx only-allow yarn",
		"husky:install": "husky install",
		"git:config": "git config --local include.path ../.gitconfig",
		"prepare": "is-ci || run-p husky:install git:config",
		"start:dev": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "run-p ts-lint styelint",
		"eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"eslint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
		"stylelint": "stylelint \"src/**/*.scss\"",
		"stylelint:fix": "stylelint \"src/**/*.scss\" --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"depcheck": "depcheck --config .depcheckrc.json",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@testing-library/jest-dom": "^5.15.1",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^12.8.3",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.6",
		"i18next": "^21.5.4",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-i18next": "^11.14.3",
		"react-scripts": "4.0.3",
		"web-vitals": "^1.1.2"
	},
	"devDependencies": {
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.11",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"eslint-plugin-react": "^7.27.1",
		"husky": "8.0.1",
		"hygen": "^6.1.0",
		"node-sass": "^6.0.1",
		"npm-run-all": "^4.1.5",
		"postcss": "8",
		"prettier": "^2.5.0",
		"stylelint": "^14.3.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard-scss": "^3.0.0",
		"typescript": "^4.5.2"
	}
}
